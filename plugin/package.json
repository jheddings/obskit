{
    "name": "obskit-plugin",
    "version": "0.0.1",
    "description": "Sample plugin for Obsidian.",
    "author": "Jason Heddings",
    "license": "MIT",
    "main": "main.js",
    "scripts": {
        "dev": "node esbuild.config.mjs",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "format": "prettier --write .",
        "format:check": "prettier --check .",
        "clean": "rm -f main.js",
        "clobber": "npm run clean && rm -Rf node_modules",
        "preflight": "npm run format && npm run lint:fix",
        "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production"
    },
    "keywords": [
        "obsidian",
        "plugin"
    ],
    "devDependencies": {
        "@types/node": "^22.18.11",
        "@typescript-eslint/eslint-plugin": "^8.46.2",
        "@typescript-eslint/parser": "^8.46.2",
        "builtin-modules": "5.0.0",
        "esbuild": "0.25.11",
        "obsidian": "^1.10.0",
        "tslib": "2.8.1",
        "typescript": "5.9.3"
    }
}
